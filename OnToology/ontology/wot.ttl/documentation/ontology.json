}, {
  "@id" : "http://iot.linkeddata.es/def/wot#Security",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The (optional) security field can be used to provide access metadata (self-contained) information of the Thing for securely transmitting information via all its resources. Definition taken from http://w3c.github.io/wot/current-practices/wot-practices#security"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Security"
  } ],
  "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status" : [ {
    "@language" : "en",
    "@value" : "testing"
  } ]
}, {
  "@id" : "http://iot.linkeddata.es/def/wot#Thing",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "An thing is anything (both physical and non-physical) which has a distinct and independent existence. Every thing has a unique identity. (Definition adapted from \nISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA))"
  }, {
    "@language" : "en",
    "@value" : "The abstract concept of a physical entity that can either be a real-world artifact, such as a device, or a virtual entity that represents physicality, such as a room or group of devices"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Thing"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:genid15"
  }, {
    "@id" : "_:genid16"
  } ]
